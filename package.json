{
  "name": "typescript-prisma",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "start:dev": "npx nodemon",
    "build": "rimraf ./build && tsc",
    "start": "npm run migrate:up && node build/index.js",
    "start:prodjs": "npm run build && npm run start",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "start:prod": "pm2 start npm --name afro-tech-prod  -- start",
    "seed:db": "cross-env NODE_ENV='seed' ts-node src/seeders",
    "migrate:up": "npx prisma db push --accept-data-loss",
    "migrate:reset": "npx prisma migrate reset --force",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "prisma": "^5.21.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "@sendgrid/mail": "^8.1.4",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "json-bigint": "^1.0.0",
    "jsonwebtoken": "^9.0.2"
  }
}
